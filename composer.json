{
	"name": "oceancodex/wpsp",
	"version": "8.0.0",
	"description": "WordPress Starter Plugin",
	"keywords": [
		"wordpress plugin",
		"wordpress starter plugin",
		"starter plugin",
		"starter",
		"wordpress",
		"plugin",
		"oceancodex",
		"wpsp",
		"wordpress framework",
		"wordpress plugin framework"
	],
	"license": "MIT",
	"require": {
		"php": "^8.0",
		"ext-json": "*",
		"ext-intl": "*",
		"ext-mbstring": "*",
		"oceancodex/wpsp-core": "^8.0.0"
	},
	"require-dev": {
	},
	"config": {
		"sort-packages": true,
		"optimize-autoloader": true,
		"autoloader-suffix": "_wpsp_1719507007"
	},
	"autoload": {
		"psr-4": {
			"WPSP\\": "",
			"Illuminate\\Foundation\\": "app/Extras/Instances/Illuminate/Foundation"
		},
		"files": [
			"app/Helpers/Helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"WPSP\\Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "OceanCodex",
			"email": "contact@oceancodex.com"
		}
	],
	"scripts": {
		"post-autoload-dump": [
			"php bin/wpsp route:remap"
		],
		"post-root-package-install": [
		],
		"post-create-project-cmd": [
			"php bin/scripts.php"
		]
	}
}
