{
	"name": "oceancodex/wpsp",
	"version": "8.4.0",
	"description": "WordPress Starter Plugin",
	"keywords": [
		"wordpress plugin",
		"wordpress starter plugin",
		"starter plugin",
		"starter",
		"wordpress",
		"plugin",
		"oceancodex",
		"wpsp",
		"wordpress framework",
		"wordpress plugin framework"
	],
	"license": "MIT",
	"require": {
		"php": "^8.4",
		"laravel/sanctum": "^4.0",
		"oceancodex/wpsp-core": "^8.4.0",
		"spatie/laravel-permission": "^6.23"
	},
	"require-dev": {
		"fakerphp/faker": "^1.23",
		"laravel/pail": "^1.2.2",
		"laravel/pint": "^1.24",
		"laravel/sail": "^1.41",
		"mockery/mockery": "^1.6",
		"nunomaduro/collision": "^8.6",
		"phpunit/phpunit": "^11.5.3"
	},
	"repositories": [
		{
			"url": "../wpsp-core",
			"type": "path",
			"options": {
				"symlink": true
			}
		}
	],
	"autoload": {
		"psr-4": {
			"WPSP\\": "",
			"WPSP\\App\\": "app/",
			"WPSP\\Database\\Factories\\": "database/factories/",
			"WPSP\\Database\\Migrations\\": "database/migrations/",
			"WPSP\\Database\\Seeders\\": "database/seeders/"
		},
		"files": [
			"app/Helpers/Helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"WPSP\\Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "OceanCodex",
			"email": "contact@oceancodex.com"
		}
	],
	"scripts": {
		"dev": [],
		"test": [],
		"setup": [],
		"post-autoload-dump": [],
		"post-root-package-install": [],
		"post-create-project-cmd": []
	},
	"config": {
		"sort-packages": true,
		"preferred-install": "dist",
		"allow-plugins": {
			"pestphp/pest-plugin": true,
			"php-http/discovery": true
		},
		"autoloader-suffix": "_wpsp_1719507007"
	},
	"minimum-stability": "stable",
	"prefer-stable": true
}
